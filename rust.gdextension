[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://rust/target/debug/libtutorial.so"
linux.release.x86_64 =   "res://rust/target/release/libtutorial.so"
windows.debug.x86_64 =   "res://rust/target/debug/tutorial.dll"
windows.release.x86_64 = "res://rust/target/release/tutorial.dll"
macos.debug =            "res://rust/target/debug/libtutorial.dylib"
macos.release =          "res://rust/target/release/libtutorial.dylib"
macos.debug.arm64 =      "res://rust/target/debug/libtutorial.dylib"
macos.release.arm64 =    "res://rust/target/release/libtutorial.dylib"

